            {'model_house/house.000.pgm', 'model_house/house.003.pgm'}, ...
            {'im1.png','im2.png'}, ...
            {'img1.jpg', 'img2.jpg'}, ...
            {'shed1.jpg', 'shed2.jpg'}, ...
            {'valbonne/002.tif', 'valbonne/011.tif'}, ...
            {'valbonne/003.tif', 'valbonne/004.tif'}, ...
            {'wadham/003.jpg', 'wadham/005.jpg'}, ...
            {'washA.png', 'washB.png'}, ...
            {'wallA.jpg', 'wallB.jpg'} ...
        };

    cvdb_ins_stereo_set(conn, 'Arandjelovic', image_set_path, arandjelovic_image_set);
end