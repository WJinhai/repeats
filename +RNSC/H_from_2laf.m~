function [H,inl] = H_from_2laf(u,T)
u2 = [ reshape(u(1:9,:),3,[]); ...
       reshape(u(10:18,:),3,[]) ];
[H,inl0] = ransacH(u2,T,int32(4),0.99);
inl = all(reshape(inl0,3,[]));

if sum(inl) > 2
    if is_model_degen(u2(1:3,inl),H)
        H = [];
        inl = [];
    end
else
    H = [];
    inl = [];
end