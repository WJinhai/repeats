function H = H_from_2p2l(u,l)
u = renormI(v(1:3,:));
u0 = renormI(v(4:6,:));

m = size(u,2);
z = zeros(m,3);
M = [u' z  bsxfun(@times,-u0(1,:),u)'; ...
     z  u' bsxfun(@times,-u0(2,:),u)'];  

S = eye(size(M,2),size(M,2));
if m > 4
    S = diag(1./max(abs(M)));
end
invS = diag(1./diag(S));

[U S V] = svd(M*S);
H = { reshape(invS*V(:,end),3,3)' };